{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext", "DOM"],
		"outDir": "dist",
		"noImplicitAny": false
	},
	"include": ["src/**/*"]
}
