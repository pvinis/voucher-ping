{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"lib": ["ESNext", "DOM"],
		"outDir": "dist"
	},
	"include": ["src/**/*", "vite.config.ts"]
}
